{
  "name": "flight_finder_api",
  "version": "1.0.0",
  "description": "flight api services",
  "main": "index.js",
  "scripts": {
    "compile": "tsc *.ts --outDir js",
    "compile:watch": "npm run compile -- --watch *.ts",
    "test": "jest --watch --silent --coverage",
    "server": "nodemon server.ts",
    "resetdata": "rimraf data",
    "docker:start": "docker compose up",
    "seeder": "node containerConfig/dbSeeder.js",
    "docker:close": "docker compose down",
    "docker:init": "npm run resetdata && npm run docker:start"
  },
  "author": "Andrea Agosta",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "nodemon": "^2.0.20",
    "dotenv": "^16.0.3",
    "pg": "^8.8.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.23",
    "@types/supertest": "^2.0.12",
    "@types/pg": "^8.6.6",
    "mocha": "^9.2.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  }
}